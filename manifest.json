{
	"manifest_version": 2,
	"name": "SocialMedia Launcher",
	"description": "Quick launch of social media apps",
	"version": "1.0.0",
	"icons": { "128": "icon_128.png" },
	"browser_action": {
		"defaul_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": ["activeTab"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"css": ["index.css"]
		}
	]
}
